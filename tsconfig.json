{
    "compilerOptions": {
        "allowJs": true,
        "target": "ES2020",
        "module": "ES2020",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "dist",
        "sourceMap": true
    },
    "include": ["arquivo/frontend_src/**/*"],
    "exclude": ["node_modules"]
}